<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="view_libreria_usuario_tree" model="ir.ui.view">
        <field name="name">libreria.usuario.tree</field>
        <field name="model">libreria_usuario</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <field name="email"/>
                <field name="phone"/>
                <field name="registration_date"/>
            </tree>
        </field>
    </record>

    <record id="action_libreria_usuarios" model="ir.actions.act_window">
        <field name="name">Usuarios</field>
        <field name="res_model">libreria_usuario</field>
        <field name="view_mode">tree,form</field>
    </record>
</odoo>